/*eslint-disable*/
function alphabetPosition(text) {
  var alph = "abcdefghijklmnopqrstuvwxyz";
  var arr = [];
  var result = []

  for (let i = 0; i < text.length; i++) {
    if (text.charAt(i) != " ") {
      arr.push(text.charAt(i));
    }
  }
  for (let j = 0; j < arr.length; j++) {
    for (let k = 0; k < alph.length; k++) {
        if (arr[j].toLowerCase() == alph[k]) {
            result.push(k+1)
        }
    }
  }

  return result.join(" ")
}

console.log(alphabetPosition("Tbc"));
